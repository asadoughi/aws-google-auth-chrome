{
    "name": "aws-google-auth",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "AssumeRoleWithSAML",
    "background": {
	"scripts": [
	    "background.js"
	],
	"persistent": true
    },
    "browser_action": {
	"default_title": "aws-google-auth"
    },
    "permissions": [
	"https://signin.aws.amazon.com/saml",
	"tabs",
	"storage"
    ],
    "options_page": "options.html"
}
